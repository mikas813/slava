{
  "name": "backend",
  "version": "1.0.0",
  "description": "Backend server for slp app",
  "main": "index.js",
  "engine": {
    "node": "14.4.0",
    "npm": "6.14.6"
  },
  "type": "module",
  "scripts": {
    "start": "node index.js",
    "backend": "nodemod index.js",
    "frontend": "npm run front --prefix client",
    "dev": "concurrently \"npm run backend\" \"npm run start --prefix client\"",
    "heroku-postbuild": "set NODE_ENV=production && cd client && npm install && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mikas813/SLP.git"
  },
  "keywords": [
    "backend",
    "db"
  ],
  "author": "Mikas813",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mikas813/SLP/issues"
  },
  "homepage": "https://github.com/mikas813/SLP#readme",
  "dependencies": {
    "aws-sdk": "^2.756.0",
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.10.0",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.4",
    "@babel/node": "^7.10.5",
    "@babel/preset-env": "^7.11.0",
    "nodemon": "^2.0.4"
  }
}
